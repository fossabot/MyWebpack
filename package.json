{
    "name": "mywebpack",
    "version": "0.0.1",
    "description": "My webpack integrated CLI for multi-entries and collaborative projects",
    "main": "./lib/test.js",
    "types": "./lib/index.d.ts",
    "repository": "git@github.com:thundernet8/MyWebpack.git",
    "author": "wuxueqian <wuxueqian2010@hotmail.com>",
    "license": "MIT",
    "bin": {
        "mpk": "./bin/mpk"
    },
    "scripts": {
        "build": "tsc",
        "lint": "tslint -e node_modules typings -c .tslintrc.json src/**/*.ts",
        "prepublish": "npm run build",
        "test": "rimraf dist && rimraf lib && yarn build && cross-env NODE_ENV=production ./bin/mpk publish -c tests/mpk.config.js",
        "test:dev": "rimraf dist && rimraf lib && yarn build && cross-env NODE_ENV=_test ./bin/mpk start -c tests/mpk.config.js",
        "test2": "rimraf dist && rimraf lib && webpack && webpack-dev-server"
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "lint-staged:ts"
        ]
    },
    "pre-commit": "lint-staged",
    "dependencies": {
        "assets-webpack-plugin": "^3.5.1",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "colors": "^1.1.2",
        "copy-webpack-plugin": "^4.2.1",
        "cross-env": "^5.1.1",
        "css-loader": "^0.28.7",
        "customized-progress-webpack-plugin": "^0.0.3",
        "express": "^4.16.2",
        "extract-text-webpack-plugin": "^3.0.2",
        "gutil": "^1.6.4",
        "html-webpack-plugin": "^2.30.1",
        "less": "^2.7.3",
        "less-loader": "^4.0.5",
        "optimize-css-assets-webpack-plugin": "^3.2.0",
        "postcss-loader": "^2.0.9",
        "prettier": "^1.8.2",
        "react-hot-loader": "^3.1.3",
        "style-loader": "^0.19.0",
        "ts-loader": "^3.1.1",
        "webpack": "^3.8.1",
        "webpack-bundle-analyzer": "^2.9.1",
        "webpack-dev-middleware": "^1.12.1",
        "webpack-dev-server": "^2.9.4",
        "webpack-hot-middleware": "^2.20.0",
        "webpack-merge": "^4.1.1",
        "webpackstablechunkid": "^0.2.0"
    },
    "devDependencies": {
        "@types/node": "^8.0.53",
        "@types/webpack": "^3.8.1",
        "@types/yargs": "^8.0.2",
        "lint-staged": "^5.0.0",
        "react": "^16.1.1",
        "rimraf": "^2.6.2",
        "typescript": "^2.6.1",
        "yargs": "^10.0.3"
    }
}
